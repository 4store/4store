#!

echo "# before insert"
../../frontend/4s-query $1 '
SELECT ?p ?o
WHERE { <foo:a> ?p ?o }' | sort
../../frontend/4s-import $1 -m http://example.com/nasty.ttl ../../../data/nasty.ttl 1>&2 > /dev/null
echo "# after insert"
../../frontend/4s-query $1 '
SELECT ?p ?o
WHERE { <foo:a> ?p ?o }' | sort
../../frontend/4s-delete-model $1 http://example.com/nasty.ttl 1>&2 > /dev/null
echo "# after delete"
../../frontend/4s-query $1 '
SELECT ?p ?o
WHERE { <foo:a> ?p ?o }' | sort
