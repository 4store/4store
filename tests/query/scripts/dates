#!

# tests dates

$TESTPATH/frontend/4s-query $1 '
SELECT ?o ?v
WHERE { GRAPH <http://example.com/dates.ttl> 
{ ?o <test:date> ?v }} 
ORDER BY ?v'

$TESTPATH/frontend/4s-query $1 '
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
ASK WHERE { GRAPH <http://example.com/dates.ttl> { 
<g> <test:date> ?date1.
<h> <test:date> ?date2.
FILTER ( ?date1 = ?date2 ) .}}'

$TESTPATH/frontend/4s-query $1 '
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
ASK WHERE { GRAPH <http://example.com/dates.ttl> { 
<a> <test:date> ?date1.
<h> <test:date> ?date2.
FILTER ( ?date1 < ?date2 ) .}}'

$TESTPATH/frontend/4s-query $1 '
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
ASK WHERE { GRAPH <http://example.com/dates.ttl> { 
<a> <test:date> ?date1.
<h> <test:date> ?date2.
FILTER ( ?date1 <= ?date2 ) .}}'

$TESTPATH/frontend/4s-query $1 '
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
ASK WHERE { GRAPH <http://example.com/dates.ttl> { 
<a> <test:date> ?date1.
FILTER ( ?date1 < "2010-03-09T00:00:01Z"^^xsd:dateTime  ) .}}'
 
